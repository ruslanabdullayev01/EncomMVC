@model Setting
<h1>@language.Getkey("SettingDetails")</h1>

<p><strong>@language.Getkey("Key"):</strong></p>


<div style="display:flex;"><strong>@language.Getkey("Value"): </strong> @Html.Raw(Model.Value)</div>

<a class="btn btn-secondary" id="btnBack" style="cursor:pointer;">@language.Getkey("GoToBack")</a>

@section Scripts {
    <script>
        document.getElementById("btnBack").onclick = function () {
            history.back();
            return false;
        };
    </script>
}